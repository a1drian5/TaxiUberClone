<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TaxiClone" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#E1C519</item>
        <item name="colorPrimaryVariant">#E1C519</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#E1C519</item>
        <item name="colorSecondaryVariant">#E1C519</item>
        <item name="colorOnSecondary">#E1C519</item>
        <item name="buttonStyle">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#E1C519</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="TaxiFull" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <item name="colorPrimary">#E1C519</item>
        <item name="colorPrimaryVariant">#E1C519</item>
        <item name="colorOnPrimary">#E1C519</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#E1C519</item>
    </style>

    <style name="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">false</item>
    </style>

    <style name="ThemeBar" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
    </style>

    <style name="PopupOverlap" parent="ThemeOverlay.AppCompat.Light" />




</resources>